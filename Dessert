

public class Dessert {
    private String type;
    private String name;
    public Dessert(String type, String name){
        this.type = type;
        this.name = name;
    }

    public String getType() {
        return type;
    }
    public void setType(String type) {
        this.type = type;
    }
    public String getName() {
        return name;
    }
    public void setName(String name) {
        this.name = name;
    }

    public double getCost(){
        return 6.00;
    }

    @Override
    public String toString(){
        return "Enjoy your dessert!!!";
    }
}
